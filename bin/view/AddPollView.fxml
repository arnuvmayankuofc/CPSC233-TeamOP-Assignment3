<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.Pane?>

<Pane prefHeight="360.0" prefWidth="438.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.AddPollController">
   <children>
      <ChoiceBox fx:id="PollPlacementChoice" layoutX="203.0" layoutY="154.0" prefWidth="150.0" />
      <Button fx:id="clearButton" layoutX="169.0" layoutY="229.0" mnemonicParsing="false" onAction="#ClearInfo" text="Clear" />
      <Button fx:id="AddPollButton" layoutX="242.0" layoutY="229.0" mnemonicParsing="false" onAction="#AddPoll" text="Add Poll" />
      <Label fx:id="PollPlacement" layoutX="84.0" layoutY="159.0" prefHeight="17.0" prefWidth="160.0" text="Select Poll Placement:" />
      <Label fx:id="PollNameText" layoutX="84.0" layoutY="112.0" text="Name of Poll to add:" />
      <TextField fx:id="UserInput" layoutX="193.0" layoutY="107.0" />
      <Label fx:id="errorMessage" alignment="CENTER" layoutX="69.0" layoutY="190.0" prefHeight="39.0" prefWidth="323.0" text="You must create a poll list before you can add a poll. Go to the Setup Poll Tracker tab" textAlignment="CENTER" wrapText="true" />
      <Label fx:id="selectionError" alignment="TOP_CENTER" layoutX="100.0" layoutY="40.0" maxWidth="-Infinity" prefHeight="35.0" prefWidth="218.0" textAlignment="CENTER" wrapText="true" />
   </children>
</Pane>
